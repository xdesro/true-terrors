---js
{
    backLink: '/writing',
    backLinkText: 'All articles',
    tags: ['page'],
    eleventyComputed: {
        footerLinks: (data) => [
            ...data.footerLinks,
            { name: "Articles", url: "/writing" },
            { name: "Notes", url: "/notes" },
        ],
        og: function ({page}) {
            const {fileSlug} = page
            const ogTargetUrl = `https://dev.henry.codes/opengraph/${fileSlug}`;
            const ogScreenshotUrl = `https://screenshot.henry.codes/${encodeURIComponent(ogTargetUrl)}/opengraph`;
            return ogScreenshotUrl;
        },
    },
}
---
{% from './_includes/article-block.njk' import articleBlock %}
{# {% renderTemplate "webc" %}
    <page-nav></page-nav>
{% endrenderTemplate %} #}

<header class="page-header">
    <div class="page-header__inner">
        <h1 class="page-header__title">Notes</h1>
        {# <img class="page-header__image page-header__image--first page-header__image--scroll" src="/img/scroll.svg"> #}
        <p class="page-header__description">Posts not-quite-ready-for-prime-time. Code snippets & brave beginnings of ideas. You can <a href="/rss/notes.xml">subscribe to this feed via RSS</a>.
    </div>
</header>

<section class="writing-section" id="notes">
    <div class="container">
        <div class="articles-list">
            {% for article in collections['note'] | sortedByPublishDate %}
                {{ articleBlock(article) }}
            {% endfor %}
        </div>
    </div>
</section>
